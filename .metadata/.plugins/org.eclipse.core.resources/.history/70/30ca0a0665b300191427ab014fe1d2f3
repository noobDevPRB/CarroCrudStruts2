<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>

	<package name="default" extends="struts-default">
	
		<action name="paginaCadastroCarro" class="controller.action.CadastroCarroAction"
			method="paginaCadastroCarro">
			<result name="success">/WEB-INF/jsp/cadastrarCarro.jsp</result>
			<result name="failure">/WEB-INF/jsp/</result>
		</action>
		
		<action name="testarPaginaDois" class="controller.action.ConsultarCarroAction"
			method="paginaBuscarOfertas">
			<result name="success">/WEB-INF/jsp/buscarOfertas.jsp</result>
			<result name="failure">/WEB-INF/jsp/loginfalhou.jsp</result>
		</action>
	
	</package>

</struts>