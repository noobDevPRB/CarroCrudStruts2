package controller.form;

public class CarroForm {
	
	
	private String marcaForm;
	private String modeloForm;
	private String descModeloForm;
	private int nPortasForm;
	private String AnoForm;
	private String tipoForm;
	private double valorForm;
	private int idForm;
	
	public String getMarcaForm() {
		return marcaForm;
	}
	public void setMarcaForm(String marcaForm) {
		this.marcaForm = marcaForm;
	}
	public String getModeloForm() {
		return modeloForm;
	}
	public void setModeloForm(String modeloForm) {
		this.modeloForm = modeloForm;
	}
	public String getDescModeloForm() {
		return descModeloForm;
	}
	public void setDescModeloForm(String descModeloForm) {
		this.descModeloForm = descModeloForm;
	}
	public int getnPortasForm() {
		return nPortasForm;
	}
	public void setnPortasForm(int nPortasForm) {
		this.nPortasForm = nPortasForm;
	}
	public String getAnoForm() {
		return AnoForm;
	}
	public void setAnoForm(String anoForm) {
		AnoForm = anoForm;
	}
	public String getTipoForm() {
		return tipoForm;
	}
	public void setTipoForm(String tipoForm) {
		this.tipoForm = tipoForm;
	}
	public double getValorForm() {
		return valorForm;
	}
	public void setValorForm(double valorForm) {
		this.valorForm = valorForm;
	}
	public String getIdForm() {
		return idForm;
	}
	public void setIdForm(String idForm) {
		this.idForm = idForm;
	}
	
	
}
